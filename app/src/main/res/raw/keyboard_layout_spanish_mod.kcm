}

key N {
    label:                              'N'
    base:                               'n'
    shift, capslock:                    'N'
    ralt:                               '\u201d'
    shift+ralt:                         '\u2019'
}

key M {
    label:                              'M'
    base:                               'm'
    shift, capslock:                    'M'
    ralt:                               '\u00b5'
    shift+ralt:                         '\u00ba'
}

key COMMA {
    label:                              ','
    base:                               ','
    shift:                              ';'
    ralt:                               '\u2022'
    shift+ralt:                         '\u00d7'
}

key PERIOD {
    label:                              '.'
    base:                               '.'
