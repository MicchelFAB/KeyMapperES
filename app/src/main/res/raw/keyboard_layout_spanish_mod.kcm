    ralt:                               '{'
    shift+ralt:                         '\u030c'
}

key BACKSLASH {
    label:                              '\u00c7'
    base:                               '\u00e7'
    shift, capslock:                    '\u00c7'
    ralt:                               '}'
    shift+ralt:                         '\u0306'
}

### ROW 4

key PLUS {
    label:                              '<'
    base:                               '<'
    shift:                              '>'
    ralt:                               '|'
    shift+ralt:                         '\u00a6'
}

key Z {
    label:                              'Z'
    base:                               'z'
    shift, capslock:                    'Z'
    ralt:                               '<'
    shift+ralt:                         '\u00ab'
}

key X {
    label:                              'X'
    base:                               'x'
    shift, capslock:                    'X'
    ralt:                               '>'
    shift+ralt:                         '\u00bb'
}

key C {
    label:                              'C'
    base:                               'c'
    shift, capslock:                    'C'
    ralt:                               '\u00a2'
    shift+ralt:                         '\u00a9'
}

key V {
    label:                              'V'
    base:                               'v'
    shift, capslock:                    'V'
    ralt:                               '\u201e'
