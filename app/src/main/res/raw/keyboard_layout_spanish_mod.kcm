}

key I {
    label:                              'I'
    base:                               'i'
    shift, capslock:                    'I'
    ralt:                               '\u2192'
    shift+ralt:                         '\u0131'
}

key O {
    label:                              'O'
    base:                               'o'
    shift, capslock:                    'O'
    ralt:                               '\u00f8'
    shift+ralt:                         '\u00d8'
}

key P {
    label:                              'P'
    base:                               'p'
    shift, capslock:                    'P'
    ralt:                               '\u00fe'
    shift+ralt:                         '\u00de'
}

key LEFT_BRACKET {
    label:                              '\u02cb'
    base:                               '\u0300'
    shift:                              '\u0302'
    ralt:                               '['
    shift+ralt:                         '\u02da'
}

key RIGHT_BRACKET {
    label:                              '+'
    base:                               '+'
    shift:                              '*'
    ralt:                               ']'
    shift+ralt:                         '\u00af'
}

### ROW 3

key A {
    label:                              'A'
    base:                               'a'
    shift, capslock:                    'A'
    ralt:                               '\u00e6'
    shift+ralt:                         '\u00c6'
}

key S {
    label:                              'S'
    base:                               's'
    shift, capslock:                    'S'
    ralt:                               '\u00df'
    shift+ralt:                         '\u1e9e'
}

key D {
    label:                              'D'
    base:                               'd'
    shift, capslock:                    'D'
    ralt:                               '\u00f0'
    shift+ralt:                         '\u00d0'
}

key F {
    label:                              'F'
    base:                               'f'
    shift, capslock:                    'F'
    ralt:                               '\u0111'
    shift+ralt:                         '\u00aa'
}

key G {
    label:                              'G'
    base:                               'g'
    shift, capslock:                    'G'
    ralt:                               '\u014b'
    shift+ralt:                         '\u014a'
}

key H {
    label:                              'H'
    base:                               'h'
    shift, capslock:                    'H'
    ralt:                               '\u0127'
    shift+ralt:                         '\u0126'
}

key J {
    label:                              'J'
    base:                               'j'
    shift, capslock:                    'J'
    ralt:                               '\u0309'
    shift+ralt:                         '\u031b'
}

key K {
    label:                              'K'
    base:                               'k'
    shift, capslock:                    'K'
    ralt:                               '\u03ba'
    shift+ralt:                         '&'
}

key L {
    label:                              'L'
    base:                               'l'
    shift, capslock:                    'L'
    ralt:                               '\u0142'
    shift+ralt:                         '\u0141'
}

key SEMICOLON {
    label:                              '\u00d1'
    base:                               '\u00f1'
    shift, capslock:                    '\u00d1'
    ralt:                               '\u0342'
    shift+ralt:                         '\u030b'
}

key APOSTROPHE {
    label:                              '\u00b4'
    base:                               '\u0301'
