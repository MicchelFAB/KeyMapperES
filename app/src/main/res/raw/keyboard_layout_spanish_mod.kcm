#
# Spanish (Spain) keyboard layout.
#

type OVERLAY

map key 86 PLUS

### ROW 1

key GRAVE {
    label:                              '\u00ba' 
    base:                               '\u00ba' 
    shift:                              '\u00aa' 
    ralt:                               '\\'
    shift+ralt:                         '\\'
}

key 1 {
    label:                              '1'
    base:                               '1'
    shift:                              '!'
    ralt:                               '|'
    shift+ralt:                         '\u00a1' 
}

key 2 {
    label:                              '2'
    base:                               '2'
    shift:                              '"'
    ralt:                               '@'
    shift+ralt:                         '\u215b' 
}

key 3 {
    label:                              '3'
    base:                               '3'
    shift:                              '\u00b7'
    ralt:                               '#'
    shift+ralt:                         '\u00a3' 
}

key 4 {
    label:                              '4'
    base:                               '4'
    shift:                              '$'
    ralt:                               '~'
    shift+ralt:                         '$'
}

key 5 {
    label:                              '5'
    base:                               '5'
    shift:                              '%'
    ralt:                               '\u00bd' 
    shift+ralt:                         '\u215c' 
}

key 6 {
    label:                              '6'
    base:                               '6'
    shift:                              '&'
    ralt:                               '\u00ac' 
    shift+ralt:                         '\u215d' 
}

key 7 {
    label:                              '7'
    base:                               '7'
    shift:                              '/'
    ralt:                               '\u007b' 
    shift+ralt:                         '\u215e' 
}

key 8 {
    label:                              '8'
    base:                               '8'
    shift:                              '(' 
    ralt:                               '['
    shift+ralt:                         '\u2122' 
}

key 9 {
    label:                              '9'
    base:                               '9'
    shift:                              ')'
    ralt:                               ']'
    shift+ralt:                         '\u00b1' 
}

key 0 {
    label:                              '0'
    base:                               '0'
    shift:                              '='
    ralt:                               '\u007d' 
    shift+ralt:                         '\u00b0' 
}

key MINUS {
    label:                              '\''
    base:                               '\''
    shift:                              '?'
    ralt:                               '\\'
    shift+ralt:                         '\u00bf' 
}

key EQUALS {
    label:                              '\u00a1'
    base:                               '\u00a1'
    shift:                              '\u00bf'
    ralt:                               '\u00b8'
    shift+ralt:                         '\u02db'
}

### ROW 2

key Q {
    label:                              'Q'
    base:                               'q'
    shift, capslock:                    'Q'
    ralt:                               '@'
    shift+ralt:                         '\u03a9'
}

key W {
    label:                              'W'
    base:                               'w'
    shift, capslock:                    'W'
    ralt:                               '\u017f'
    shift+ralt:                         '\u00a7'
}

key E {
    label:                              'E'
    base:                               'e'
    shift, capslock:                    'E'
    ralt:                               '\u20ac'
    shift+ralt:                         '\u00a2' 
}

key R {
    label:                              'R'
    base:                               'r'
    shift, capslock:                    'R'
    ralt:                               '\u00b6'
    shift+ralt:                         '\u00ae'
}

key T {
    label:                              'T'
    base:                               't'
    shift, capslock:                    'T'
    ralt:                               '\u0167' 
    shift+ralt:                         '\u0166' 
}

key Y {
    label:                              'Y'
    base:                               'y'
    shift, capslock:                    'Y'
    ralt:                               '\u2190'
    shift+ralt:                         '\u00a5'
}

key U {
    label:                              'U'
    base:                               'u'
    shift, capslock:                    'U'
